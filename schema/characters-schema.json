{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "characters"
    ],
    "properties": {
      "characters": {
        "$id": "#/properties/characters",
        "type": "array",
        "title": "The Characters Schema",
        "items": {
          "$id": "#/properties/characters/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "id",
            "name",
            "gender",
            "status",
            "attributes"
          ],
          "properties": {
            "id": {
              "$id": "#/properties/characters/items/properties/id",
              "type": "string",
              "title": "The Id Schema",
              "default": "",
              "examples": [
                "1"
              ],
              "pattern": "^(.*)$"
            },
            "name": {
              "$id": "#/properties/characters/items/properties/name",
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "Adam Warlock"
              ],
              "pattern": "^(.*)$"
            },
            "gender": {
              "$id": "#/properties/characters/items/properties/gender",
              "type": "string",
              "title": "The Gender Schema",
              "default": "",
              "examples": [
                "Male"
              ],
              "pattern": "^(.*)$"
            },
            "status": {
              "$id": "#/properties/characters/items/properties/status",
              "type": "string",
              "title": "The Status Schema",
              "default": "",
              "examples": [
                "Hero"
              ],
              "pattern": "^(.*)$"
            },
            "attributes": {
              "$id": "#/properties/characters/items/properties/attributes",
              "type": "object",
              "title": "The Attributes Schema",
              "required": [
                "intelligence",
                "strength",
                "speed",
                "durability",
                "energyProjection",
                "fightingAblitiy"
              ],
              "properties": {
                "intelligence": {
                  "$id": "#/properties/characters/items/properties/attributes/properties/intelligence",
                  "type": "string",
                  "title": "The Intelligence Schema",
                  "default": "",
                  "examples": [
                    "5"
                  ],
                  "pattern": "^(.*)$"
                },
                "strength": {
                  "$id": "#/properties/characters/items/properties/attributes/properties/strength",
                  "type": "string",
                  "title": "The Strength Schema",
                  "default": "",
                  "examples": [
                    "6"
                  ],
                  "pattern": "^(.*)$"
                },
                "speed": {
                  "$id": "#/properties/characters/items/properties/attributes/properties/speed",
                  "type": "string",
                  "title": "The Speed Schema",
                  "default": "",
                  "examples": [
                    "6"
                  ],
                  "pattern": "^(.*)$"
                },
                "durability": {
                  "$id": "#/properties/characters/items/properties/attributes/properties/durability",
                  "type": "string",
                  "title": "The Durability Schema",
                  "default": "",
                  "examples": [
                    "7"
                  ],
                  "pattern": "^(.*)$"
                },
                "energyProjection": {
                  "$id": "#/properties/characters/items/properties/attributes/properties/energyProjection",
                  "type": "string",
                  "title": "The Energyprojection Schema",
                  "default": "",
                  "examples": [
                    "7"
                  ],
                  "pattern": "^(.*)$"
                },
                "fightingAblitiy": {
                  "$id": "#/properties/characters/items/properties/attributes/properties/fightingAblitiy",
                  "type": "string",
                  "title": "The Fightingablitiy Schema",
                  "default": "",
                  "examples": [
                    "6"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    }
  }